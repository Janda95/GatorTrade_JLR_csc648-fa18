{
  "name": "csc648-fa18-team04",
  "version": "1.0.0",
  "description": "CSC648 Team 04 Website",
  "main": "app.js",
  "scripts": {
    "start:dev": "NODE_ENV=development DEBUG=APP_NAME:* nodemon ./bin/www",
    "start:windev": "nodemon ./bin/www",
    "start": "NODE_ENV=production nodemon ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create:migration": "sequelize migration:generate --name",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "db:create:model:user": "sequelize model:create --name user --attributes firstName:string,lastName:string,email:string,password:string,profilePicture:string",
    "db:create:model:admin": "sequelize model:create --name admin --attributes userId:integer",
    "db:create:model:item": "sequelize model:create --name item --attributes sellerId:integer,title:string,description:string,price:float,category:string,meetingPlace:string,status:string",
    "db:create:model:message": "sequelize model:create --name message --attributes itemId:integer,sellerId:integer,message:string",
    "db:create:model:image": "sequelize model:create --name image --attributes itemId:integer,imageLink:string",
    "db:create:model:category": "sequelize model:create --name category --attributes title:string",
    "db:create:model:meetingPlace": "sequelize model:create --name meetingPlace --attributes title:string"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CSC-648-SFSU/csc648-fa18-Team04.git"
  },
  "author": "Abigail Chin, Alyssa Malunao, Dhruv Shah, Jarek Rettinghouse, Jed Ahmadia, Nikita Bajracharya, Robert Quinones",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CSC-648-SFSU/csc648-fa18-Team04/issues"
  },
  "homepage": "https://github.com/CSC-648-SFSU/csc648-fa18-Team04#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-ejs-layouts": "^2.5.0",
    "express-validator": "^5.3.0",
    "http": "0.0.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.4",
    "pg": "^7.4.3",
    "sequelize": "^4.38.1",
    "sequelize-cli": "^4.1.1"
  }
}